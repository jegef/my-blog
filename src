<script>
import MySearch from './MySearch';
import MyMessage from './MyMessage';
import side_bar from './side_bar.json';
import $ from 'jquery';
import {inject,ref} from 'vue'
export default{
    components:{
        MySearch,
        MyMessage,
        
    },
    data(){
        return{
            activeIndex:null,
            list:[
                {tip:'ğŸ·ï¸',tipMessage:'æˆ‘çš„æ ‡ç­¾',isVisible: false},
                {tip:'ğŸ“–',tipMessage:'é˜…è¯»æ’è¡Œ',isVisible: false},
                {tip:'â¤ï¸',tipMessage:'æ¨èæ’è¡Œ',isVisible: false},
                {tip:'âœ‰ï¸',tipMessage:'æœ€æ–°è¯„è®º',isVisible: false},
                {tip:'ğŸ—ƒï¸',tipMessage:'éšç¬”æ¡£æ¡ˆ',isVisible: false},
                
            ]
        }
    },
   setup(){
      const sideBarData=ref(side_bar.tags);
      const articleData=ref(side_bar.readCounts);
      const showTip=()=>{
        $(document).ready(function() {
            $('.list_t').click(function() {
                var list_b = $(this).next('.list_b');
                $('.list_b').not(list_b).slideUp('slow'); // æ”¶èµ·å…¶ä»–å†…å®¹
                list_b.slideDown('slow');
              });
            $('.list_t').dblclick(function(){
              // var index=$(this).index();
              var list_b = $(this).next('.list_b');
              
              list_b.slideUp('slow');
              // $('.list_b').eq(index).css({
              //   display:'none'
              // })
            })
        });
      }
      const showSideBar=inject('showSideBar')
      const hide_side_bar=()=>{
          return showSideBar.value=!showSideBar.value,writing.value=false;
      }
      const writing=inject('writing');
      const showWritingPage=()=>{
        return writing.value=!writing.value
      }
      return{showTip,hide_side_bar,showSideBar,writing,showWritingPage,sideBarData,articleData}
   }
}
</script>
